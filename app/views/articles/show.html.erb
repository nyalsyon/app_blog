<h3>Selected article</h3>
<%= link_to 'create new article',new_article_path%>
<p>
    <h5>Title:</h5><%= @article.title%>
</p>
<p>
  <h5>Description:</h5><%=@article.description%>
</p>
<%= link_to 'Edit',edit_article_path(@article)%>
<%= link_to 'Back to all articles list', root_path(@article)%>
<%= button_to 'Delete', article_path(@article), method: :delete, action: :destroy,  onclick: 'return confirm("Are you sure?")'%>
