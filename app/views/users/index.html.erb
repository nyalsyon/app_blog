<div class="container">
  <table class="table table-striped">
    <thead>
      <tr>
        <th>
          Username
        </th>
        <th>
          Password
        </th>
        <th>
          E-mail
        </th>
      </tr>
    </thead>
    <% @user.each do |user| %>
    <tr>
      <td>
        <%= user.username %>
      </td>
      <td>
        <%= user.password %>
      </td>
      <td>
        <%= user.email %>
      </td>
      <td>
        <%= link_to 'Edit',edit_user_path(user) %>
      </td>
      <td>
        <%= link_to 'Show',user_path(user) %>
      </td>
      <td>
        <%= button_to 'Delete',user_path(user),method: :delete,action: :destroy,onclick:'return confirm:{"Are you sure?"}'%>
      </td>
    </tr>
    <% end %>

  </table>
</div>
